<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.codemaster.nsstreasurehunt">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/appz"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/appz_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.NSSTreasureHunt">
        <activity android:name=".DevScreen"></activity>
        <activity android:name=".LoginActivity" />
        <activity android:name=".FinishScreen" />
        <activity android:name=".QuizScreen" />
        <activity android:name=".HomeScreen" />
        <activity android:name=".OTPScreen" />
        <activity android:name=".CreateUser" />
        <activity
            android:name=".SplashScreen"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>